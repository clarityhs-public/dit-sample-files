            <xsd:element name="ProbSolDivRRResult" id="_4.20.A" type="hmis:noYesMissing"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Problem Solving/Diversion/Rapid Resolution
                        intervention or service result - Client housed/re-housed in a safe
                        alternative
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ReferralCaseManageAfter" id="_4.20.B" type="hmis:noYesMissing"
