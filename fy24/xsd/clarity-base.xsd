                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ReferralResult" id="_4.20.D" type="hmis:referralResult" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Referral Result</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ResultDate" id="_4.20.E" type="xsd:date" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Date of result</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
