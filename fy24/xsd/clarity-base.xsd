<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema
        targetNamespace="http://www.clarityhumanservices.com/schema/2024/1/HUD_HMIS.xsd"
        xmlns="http://www.clarityhumanservices.com/schema/2024/1/HUD_HMIS.xsd"
        xmlns:hmis="http://www.clarityhumanservices.com/schema/2024/1/HUD_HMIS.xsd"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        vc:minVersion="1.0"
        version="v1.0.7-rc.2"
        elementFormDefault="qualified"
        attributeFormDefault="qualified">

    <xsd:element name="Sources" type="hmis:sources">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">XML Root element. A description of what software
                provider and organization are sending this XML information. Many sources can be
                grouped together into one XML file.
            </xsd:documentation>
        </xsd:annotation>
    </xsd:element>

    <xsd:complexType name="affiliation">
        <xsd:sequence>
            <xsd:element name="AffiliationID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Reference to the Affiliation's unique ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ProjectID" type="hmis:projectID"/>
            <xsd:element name="ResProjectID" id="_2.02.C" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">[If Yes for “Affiliated with a residential
                        project”] Project ID(s) of residential project(s) affiliated with SSO or
                        RRH: Services Only project
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="hmis:baseMetaData"/>
    </xsd:complexType>

    <xsd:complexType name="assessment">
        <xsd:sequence>
            <xsd:element name="AssessmentID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Reference to the Assessment's unique ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EnrollmentID" type="hmis:enrollmentID"/>
            <xsd:element name="PersonalID" type="hmis:personalID"/>
            <xsd:element name="AssessmentDate" id="_4.19.1" type="xsd:date">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Date of assessment</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AssessmentLocation" id="_4.19.2" type="hmis:string250" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Assessment location [drop down]
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AssessmentType" id="_4.19.3" type="hmis:assessmentType" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Assessment Type</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AssessmentLevel" id="_4.19.4" type="hmis:assessmentLevel"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Assessment Level</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PrioritizationStatus" id="_4.19.7" type="hmis:prioritizationStatus"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Prioritization Status</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="hmis:baseMetaData"/>
    </xsd:complexType>

    <xsd:complexType name="assessmentQuestions">
        <xsd:sequence>
            <xsd:element name="AssessmentQuestionID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Reference to the Assessment Question's unique
                        ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AssessmentID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Reference to the Assessment's unique ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EnrollmentID" type="hmis:enrollmentID"/>
            <xsd:element name="PersonalID" type="hmis:personalID"/>
            <xsd:element name="AssessmentQuestionGroup" type="hmis:string250" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Locally determined</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AssessmentQuestionOrder" type="xsd:integer" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Locally determined</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AssessmentQuestion" id="_4.19.5" type="hmis:string250">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Assessment questions – Local determination - as
                        defined by the community
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AssessmentAnswer" id="_4.19.A" type="hmis:string500" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Assessment answers - Local determination –
                        responses to questions defined by community
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="hmis:baseMetaData"/>
    </xsd:complexType>

    <xsd:complexType name="assessmentResults">
        <xsd:sequence>
            <xsd:element name="AssessmentResultID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Reference to the Assessment Question's unique
                        ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AssessmentID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Reference to the Assessment's unique ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EnrollmentID" type="hmis:enrollmentID"/>
            <xsd:element name="PersonalID" type="hmis:personalID"/>
            <xsd:element name="AssessmentResultType" id="_4.19.6" type="hmis:string250">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Assessment Result Type – Local determination -
                        as defined by the community
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AssessmentResult" id="_4.19B" type="hmis:string250" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Assessment Result</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="hmis:baseMetaData"/>
    </xsd:complexType>

    <xsd:complexType name="cEParticipation">
        <xsd:sequence>
            <xsd:element name="CEParticipationID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Reference to the CE Participation's unique ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ProjectID" type="hmis:projectID">
                <xsd:annotation>
                    <xsd:documentation>Must match an hmis:project.ProjectID</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AccessPoint" id="_2.09.1" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Project is a Coordinated Entry Access Point
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PreventionAssessment" id="_2.09.A.1" type="hmis:noYes" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Homelessness Prevention Assessment, Screening,
                        and/or Referral. Provided by CE Project
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CrisisAssessment" id="_2.09.A.2" type="hmis:noYes" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Shelter Assessment, Screening, and/or Referral.
                        Provided by CE Project
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="HousingAssessment" id="_2.09.A.3" type="hmis:noYes" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Housing Assessment, Screening, and/or Referral.
                        Provided by CE Project
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DirectServices" id="_2.09.A.4" type="hmis:noYes" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Direct Services (search and/or placement
                        support). Provided by CE Project
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ReceivesReferrals" id="_2.09.2" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Project Receives CE Referrals. Provided by CE
                        Project
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CEParticipationStatusStartDate" id="_2.09.3" type="xsd:date">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">CE Participation Status Start Date ([date
                        field])
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CEParticipationStatusEndDate" id="_2.09.4" type="xsd:date"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">CE Participation Status End Date ([date
                        field])
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="hmis:baseMetaData"/>
    </xsd:complexType>

    <xsd:complexType name="client">
        <xsd:sequence>
            <xsd:element name="PersonalID" type="hmis:personalID"/>
            <xsd:element name="FirstName" id="_3.01.1" type="hmis:nameHashingOption" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">First ([text])</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MiddleName" id="_3.01.2" type="hmis:nameHashingOption" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Middle ([text])</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LastName" id="_3.01.3" type="hmis:nameHashingOption" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Last (text)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="NameSuffix" id="_3.01.4" type="hmis:string50" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Suffix ([text])</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="NameDataQuality" id="_3.01.5" type="hmis:nameDataQuality">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Name Data Quality</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SSN" id="_3.02.1" type="hmis:ssnHashingOption" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Social Security Number</xsd:documentation>
                    <xsd:documentation xml:lang="en">Unknown SSN digits should be indicated with a
                        lower case "x".
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SSNDataQuality" id="_3.02.2" type="hmis:ssnDataQuality">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">SSN Data Quality</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DOB" id="_3.03.1" type="xsd:date" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Date of Birth</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DOBDataQuality" id="_3.03.2" type="hmis:dobDataQuality">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">DOB Data Quality</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AmIndAKNative" id="_3.04.1.1" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">American Indian, Alaska Native, or Indigenous
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Asian" id="_3.04.1.2" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Asian or Asian American</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="BlackAfAmerican" id="_3.04.1.3" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Black, African American, or African
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="HispanicLatinaeo" id="_3.04.1.4" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Hispanic/Latina/e/o</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MidEastNAfrican" id="_3.04.1.5" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Middle Eastern or North African
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="NativeHIPacific" id="_3.04.1.6" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Native Hawaiian or Pacific Islander
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="White" id="_3.04.1.7" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">White</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="RaceNone" id="_3.04.1.8" type="hmis:raceGenderNone" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Race None. Non-null only if all other race
                        fields = 0.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AdditionalRaceEthnicity" id="_3.04.2" type="hmis:string100"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Additional Race and Ethnicity Detail ([Text])
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Woman" id="_3.06.1.1" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Woman (Girl, if child)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Man" id="_3.06.1.2" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Man (Boy, if child)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="NonBinary" id="_3.06.1.3" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Non-Binary</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CulturallySpecific" id="_3.06.1.4" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Culturally Specific Identity (e.g., TwoSpirit)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Transgender" id="_3.06.1.5" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Transgender</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Questioning" id="_3.06.1.6" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Questioning</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DifferentIdentity" id="_3.06.1.7" type="hmis:noYes">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Different Identity</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="GenderNone" id="_3.06.1.8" type="hmis:raceGenderNone" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Gender None. Non-null only if all other gender
                        fields = 0.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DifferentIdentityText" id="_3.06.A" type="hmis:string100"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">If Different Identity, Please Specify ([Text]).
                        Non-null if DifferentIdentity=1
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="VeteranStatus" id="_3.07.1" type="hmis:noYesReasonsForMissingData">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Veteran Status</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="hmis:baseMetaData"/>
    </xsd:complexType>
    <xsd:complexType name="clientVeteranInfo">
        <xsd:sequence>
            <xsd:element name="ClientVeteranInfoID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Reference to the ClientVeteranInfo's unique ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PersonalID" type="hmis:personalID"/>
            <xsd:element name="YearEnteredService" id="_V1.1" type="xsd:gYear" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Year Entered Military Service (year). Values
                        between 1920 and the current year.
                        ^(19[2-8][0-9]|199[0-9]|20[01][0-9]|202[01])$
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="YearSeparated" id="_V1.2" type="xsd:gYear" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Year Separated from Military Service (year).
                        ^(19[2-8][0-9]|199[0-9]|20[01][0-9]|202[01])$$
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="WorldWarII" id="_V1.3" type="hmis:noYesReasonsForMissingData"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Theatre of Operations: World War II
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="KoreanWar" id="_V1.4" type="hmis:noYesReasonsForMissingData"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Theatre of Operations: Korean War
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="VietnamWar" id="_V1.5" type="hmis:noYesReasonsForMissingData"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Theatre of Operations: Vietnam War
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DesertStorm" id="_V1.6" type="hmis:noYesReasonsForMissingData"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Theatre of Operations: Persian Gulf War
                        (Operation Desert Storm)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AfghanistanOEF" id="_V1.7" type="hmis:noYesReasonsForMissingData"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Theatre of Operations: Afghanistan (Operation
                        Enduring Freedom)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="IraqOIF" id="_V1.8" type="hmis:noYesReasonsForMissingData"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Theatre of Operations: Iraq (Operation Iraqi
                        Freedom)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="IraqOND" id="_V1.9" type="hmis:noYesReasonsForMissingData"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Theatre of Operations: Iraq (Operation New
                        Dawn)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="OtherTheater" id="_V1.10" type="hmis:noYesReasonsForMissingData"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Theatre of Operations: Other Peace-keeping
                        Operations or Military Interventions (such as Lebanon, Panama, Somalia,
                        Bosnia, Kosovo)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MilitaryBranch" id="_V1.11" type="hmis:militaryBranch" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Branch of the Military</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DischargeStatus" id="_V1.12" type="hmis:dischargeStatus"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Discharge Status</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="hmis:baseMetaData"/>
    </xsd:complexType>

    <xsd:complexType name="connectionWithSOAR">
        <xsd:sequence>
            <xsd:element name="ConnectionWithSOARID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Reference to the connectionWithSOAR's unique
                        ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EnrollmentID" type="hmis:enrollmentID"/>
            <xsd:element name="ConnectionWithSOAR" id="_P4.1" type="hmis:noYesReasonsForMissingData">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Connection with SOAR</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute ref="hmis:dataCollectionStage" use="required"/>
        <xsd:attribute ref="hmis:informationDate" use="required"/>
        <xsd:attributeGroup ref="hmis:baseMetaData"/>
    </xsd:complexType>

    <xsd:complexType name="currentLivingSituation">
        <xsd:sequence>
            <xsd:element name="CurrentLivingSitID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Reference to the currentLivingSituation's unique
                        ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EnrollmentID" type="hmis:enrollmentID"/>
            <xsd:element name="PersonalID" type="hmis:personalID"/>
            <xsd:element name="CurrentLivingSituation" id="_4.12.2" type="hmis:livingSituation">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Current Living Situation [See Appendix A –
                        Living Situation Information]
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CLSSubsidyType" id="_4.12.A" type="hmis:rentalSubsidyTypes"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Rental Subsidy Type [See Appendix A]. Null
                        unless CurrentLivingSItuation = 435.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="VerifiedBy" id="_4.12.3" type="hmis:string100" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Living situation verified by [list of Continuum
                        projects]. Null unless ProjectType = 14.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LeaveSituation14Days" id="_4.12.B"
                        type="hmis:noYesReasonsForMissingData" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Is client going to have to leave their current
                        living situation within 14 days?
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SubsequentResidence" id="_4.12.C"
                        type="hmis:noYesReasonsForMissingData" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Has a subsequent residence been identified? Null
                        unless LeaveSituation14Days = 1.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ResourcesToObtain" id="_4.12.D" type="hmis:noYesReasonsForMissingData"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Does individual or family have resources or
                        support networks to obtain other permanent housing?
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LeaseOwn60Day" id="_4.12.E" type="hmis:noYesReasonsForMissingData"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Has the client had a lease or ownership interest
                        in a permanent housing unit in the last 60 days?
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
