    <xsd:complexType name="serviceFAReferral">
        <xsd:sequence>
            <xsd:element name="ServiceFAReferralID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Reference to the Services' unique ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EnrollmentID" type="hmis:enrollmentID"/>
            <xsd:element name="DateProvided" type="xsd:date">
            </xsd:element>
            <xsd:element name="RecordType" type="hmis:recordType">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">
                        4.14 Bed Night (RecordType 200)
                        P1 Services Provided – PATH (RecordType 141)
                        P2 Referrals Provided – PATH (RecordType 161)
                        R14 RHY Service Connections (RecordType 142)
                        W1 Services Provided – HOPWA (RecordType 143)
                        W2 Financial Assistance – HOPWA (RecordType 151)
                        V2 Services Provided – SSVF (RecordType 144)
                        V3 Financial Assistance – SSVF (RecordType 152)
                        V8.1 HUD-VASH Voucher Tracking (RecordType 210)
                        C2.2 Moving On Assistance (RecordType 300)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="TypeProvided" type="hmis:typeProvidedBase"/>
            <xsd:element name="OtherTypeProvided" type="hmis:string50" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">See FY2024 HMIS Data Dictionary, Sections V2.D
                        and V8.A. Null unless RecordType = 144 and TypeProvided = 6, or RecordType =
                        210 and TypeProvided = 12.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="MovingOnOtherType" id="_C2.A" type="hmis:string50" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Other (please specify)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SubTypeProvided" type="hmis:subTypeProvidedBase" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">See FY2024 HMIS Data Dictionary, Sections V2.A,
                        V2.B, and V2.C. Null unless RecordType = 144 and TypeProvided = 3, 4, or 5.
                        For TypeProvided: 3 – list V2.A, TypeProvided: 4 – list V2.B, TypeProvided:
                        5 – list V2.C
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="FAAmount" type="hmis:fAAmountBase" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">See FY2024 HMIS Data Dictionary, Section W2 #3
                        and V3 #2, Financial Assistance Amount. Null unless RecordType = 151 or 152.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">In the instance document, the xsi:type of this
                        element must be declared, and the type depends on the value within the
                        sibling hmis:DisabilityType element. See the example HUD_HMIS_Instance.xml
                        file to see how this should appear. Either
                        xsi:type="hmis:disabilityResponse[insert type]" or
                        xsi:type="hmis:disabilityResponseSubstanceUseDisorder"can be used.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="FAStartDate" id="_V3.1" type="xsd:date" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Start Date of Financial Assistance ([date
