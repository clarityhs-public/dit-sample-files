                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Not in HMIS Data Dictionary</xsd:documentation>
                    <xsd:documentation xml:lang="en">The phone number of the user if available.
                        Limited to 10 digits / no punctuation. [2-9][0-9]{2}[2-9][0-9]{2}[0-9]{4}
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="UserExtension" type="hmis:string5" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Not in HMIS Data Dictionary</xsd:documentation>
                    <xsd:documentation xml:lang="en">The phone extension of the user, if available.
                        Limited to 5 digits / no punctuation. [0-9]{1,5}
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="UserEmail" type="hmis:string320" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Not in HMIS Data Dictionary</xsd:documentation>
                    <xsd:documentation xml:lang="en">The email address of the user, if available.
                        Valid email addresses only
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="hmis:baseMetaData"/>
    </xsd:complexType>

    <xsd:complexType name="vashExitReason">
