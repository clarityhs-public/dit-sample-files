                    <xsd:documentation xml:lang="en">Key reference to the User</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Deleted" type="xsd:nonNegativeInteger" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Deleted</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AgencyDeletedId" type="hmis:string32" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Key reference to the Agency</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="hmis:baseMetaData"/>
