    <xsd:complexType name="exitRHY">
        <xsd:sequence>
            <xsd:element name="ExitRHYID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Reference to the ExitRHY' unique ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ExitID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Key reference to the Enrollment
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ProjectCompletionStatus" id="_R17.1"
                        type="hmis:projectCompletionStatus" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Project Completion Status</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EarlyExitReason" id="_R17.A" type="hmis:earlyExitReason"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Select the major reason</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ExchangeForSex" id="_R15.1" type="hmis:noYesReasonsForMissingData"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Ever received anything in exchange for sex (e.g.
                        money, food, drugs, shelter)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ExchangeForSexPastThreeMonths" id="_R15.A"
                        type="hmis:noYesReasonsForMissingData" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">If Yes for “Ever received anything in exchange
                        for sex”. In the last three months. Null unless R15.1 = 1
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CountOfExchangeForSex" id="_R15.B" type="hmis:countOfExchangeForSex"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">If Yes for “Ever received anything in exchange
                        for sex”. How many times. Null unless R15.1 = 1.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AskedOrForcedToExchangeForSex" id="_R15.C"
                        type="hmis:noYesReasonsForMissingData" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">If Yes for “Ever received anything in exchange
                        for sex”. Ever made/persuaded/forced to have sex in exchange for something.
                        Null unless R15.1 = 1.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AskedOrForcedToExchangeForSexPastThreeMonths" id="_R15.D"
                        type="hmis:noYesReasonsForMissingData" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">If Yes for “Ever made/persuaded/forced to have
                        sex in exchange for something?”. In the last three months? Null unless R15.C
                        = 1.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="WorkplaceViolenceThreats" id="_R16.1"
                        type="hmis:noYesReasonsForMissingData" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Ever afraid to quit/leave work due to threats of
                        violence to yourself, family, or friends
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="WorkplacePromiseDifference" id="_R16.2"
                        type="hmis:noYesReasonsForMissingData" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Ever promised work where work or payment was
                        different than you expected
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CoercedToContinueWork" id="_R16.A"
                        type="hmis:noYesReasonsForMissingData" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Felt forced, coerced, pressured, or tricked into
                        continuing the job
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LaborExploitPastThreeMonths" id="_R16.B"
                        type="hmis:noYesReasonsForMissingData" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">In the last 3 months</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CounselingReceived" id="_R18.1" type="hmis:noYesMissing"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Client received counseling</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="IndividualCounseling" id="_R18.A.1" type="hmis:noYesMissing"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Individual</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="FamilyCounseling" id="_R18.A.2" type="hmis:noYesMissing"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Family</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="GroupCounseling" id="_R18.A.3" type="hmis:noYesMissing" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Group – including peer counseling
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SessionCountAtExit" id="_R18.B" type="xsd:positiveInteger"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Identify the number of sessions received by
                        exit
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SessionsInPlan" id="_R18.2" type="xsd:positiveInteger" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Total number of sessions planned in youth’s
                        treatment or service plan
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PostExitCounselingPlan" id="_R18.3" type="hmis:noYesMissing"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">A plan is in place to start or continue
                        counseling after exit
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DestinationSafeClient" id="_R19.1"
                        type="hmis:noYesReasonsForMissingData" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Exit destination safe – as determined by the
                        client
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DestinationSafeWorker" id="_R19.2" type="hmis:noYesWorkerDoesntKnow"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Exit destination safe – as determined by the
                        project/caseworker
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PosAdultConnections" id="_R19.3" type="hmis:noYesWorkerDoesntKnow"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Client has permanent positive adult connections
                        outside of project
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PosPeerConnections" id="_R19.4" type="hmis:noYesWorkerDoesntKnow"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Client has permanent positive peer connections
                        outside of project
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PosCommunityConnections" id="_R19.5" type="hmis:noYesWorkerDoesntKnow"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Client has permanent positive community
                        connections outside of project
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="hmis:baseMetaData"/>
    </xsd:complexType>

    <xsd:complexType name="export">
        <xsd:sequence>
            <xsd:element name="ExportID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Reference to the Export's unique ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ExportDate" type="xsd:dateTime">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Time the export was performed.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ExportPeriod" type="hmis:dateRangeCapped">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">The date range bounding the data set being
                        transmitted. Used with ExportPeriodType to clarify what the date range
                        represents.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ExportPeriodType" type="hmis:exportPeriodType">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Pick one of the types to specify the definition
                        of export period being used in ExportPeriod.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ExportDirective" type="hmis:exportDirective">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Select one of the ExportDirective choices to
                        instruct the target as to how the data being transmitted should be handled.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ImplementationID" id="_5.10" type="hmis:string200">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Implementation ID [Vendor Generated]. The
                        purpose of Implementation ID is to indicate the scope of uniqueness of the
                        primary keys within an export. Each implementation of HMIS is unique and the
                        Implementation ID is to identify that exported data is affiliated with a
                        given HMIS implementation. For example, an HMIS vendor providing HMIS
                        software to two distinct CoCs with two distinct HMIS implementations would
                        have different Implementation IDs for those different implementations.
                        Alternatively, an HMIS vendor providing HMIS software to two distinct CoCs
                        sharing an HMIS implementation would have the same Implementation ID. A
                        unique Implementation ID must be created per HMIS instance, but there is no
                        required format.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="Affiliation" type="hmis:affiliation"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="Assessment" type="hmis:assessment"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="AssessmentQuestions" type="hmis:assessmentQuestions"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="AssessmentResults" type="hmis:assessmentResults"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="CEParticipation" type="hmis:cEParticipation"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="Client" type="hmis:client"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="ClientVeteranInfo" type="hmis:clientVeteranInfo"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="ConnectionWithSOAR" type="hmis:connectionWithSOAR"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="CurrentLivingSituation" type="hmis:currentLivingSituation"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="DateOfEngagement" type="hmis:dateOfEngagement"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="Disabilities" type="hmis:disabilities"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="HealthAndDV" type="hmis:healthAndDV"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="Education" type="hmis:education"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="Employment" type="hmis:employment"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="Enrollment" type="hmis:enrollment"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="EntryRHY" type="hmis:entryRHY"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="EntrySSVF" type="hmis:entrySSVF"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="Event" type="hmis:event"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="Exit" type="hmis:exit"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="ExitHousingAssessment" type="hmis:exitHousingAssessment"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="ExitRHY" type="hmis:exitRHY"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="Funder" type="hmis:funder"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="HealthInsurance" type="hmis:healthInsurance"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="HealthStatus" type="hmis:healthStatus"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="IncomeAndSources" type="hmis:incomeAndSources"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="Inventory" type="hmis:inventory"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="MedicalAssistance" type="hmis:medicalAssistance"  minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="MoveInDate" type="hmis:moveInDate"  minOccurs="0" maxOccurs="unbounded"/>
