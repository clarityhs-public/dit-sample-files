            <xsd:element name="PercentAMI" id="_V4.1" type="hmis:percentAMISimple" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Household Income as a Percentage of AMI
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="VAMCStation" id="_V6.1" type="hmis:vamcStation" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">VAMC Station Number</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="TargetScreenReqd" id="_V7.1" type="hmis:noYesMissing" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Is Homelessness Prevention targeting screener
                        required?
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="TimeToHousingLoss" id="_V7.A" type="hmis:timeToHousingLoss"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Current housing loss expected within…
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AnnualPercentAMI" id="_V7.B" type="hmis:annualPercentAMI"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Current household income</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="LiteralHomelessHistory" id="_V7.C" type="hmis:literalHomelessHistory"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Past experience of Homelessness
                        (street/shelter/transitional housing) (any adult)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ClientLeaseholder" id="_V7.D" type="hmis:noYesMissing" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Head of Household is not a current leaseholder/renter of
                        unit
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="HOHLeaseholder" id="_V7.E" type="hmis:noYesMissing" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation>Head of Household has never been a leaseholder/renter of
                        unit
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SubsidyAtRisk" id="_V7.F" type="hmis:noYesMissing" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Currently at risk of losing a tenant-based
                        housing subsidy or housing in a subsidized building or unit (household)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EvictionHistory" id="_V7.G" type="hmis:evictionHistory" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Rental Evictions within the past 7 years (any
                        adult)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CriminalRecord" id="_V7.H" type="hmis:noYesMissing" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Criminal record for arson, drug dealing or
                        manufacture, or felony offense against persons or property (any adult)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="IncarceratedAdult" id="_V7.I" type="hmis:incarceratedAdult"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Incarcerated as adult (any adult in household)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="PrisonDischarge" id="_V7.J" type="hmis:noYesMissing" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Discharged from jail or prison within last six
                        months after incarceration of 90 days or more (adults)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SexOffender" id="_V7.K" type="hmis:noYesMissing" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Registered sex offender (any household
                        members)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DisabledHoH" id="_V7.L" type="hmis:noYesMissing" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Head of Household with disabling condition
                        (physical health, mental health, substance use) that directly affects
                        ability to secure/maintain housing
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CurrentPregnant" id="_V7.M" type="hmis:noYesMissing" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Currently pregnant (any household member)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="SingleParent" id="_V7.N" type="hmis:noYesMissing" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Single parent/guardian household with minor
                        child(ren)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DependentUnder6" id="_V7.O" type="hmis:dependentUnder6" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Household includes one or more young children
                        (age six or under), or a child who requires significant care
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="HH5Plus" id="_V7.P" type="hmis:noYesMissing" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Household size of 5 or more requiring at least 3
                        bedrooms (due to age/gender mix)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="CoCPrioritized" id="_V7.Q" type="hmis:noYesMissing" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Current/recent resident in area prioritized by
                        the CoC
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="HPScreeningScore" id="_V7.R" type="xsd:unsignedInt" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">HP applicant total points (integer)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ThresholdScore" id="_V7.S" type="xsd:positiveInteger" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Grantee targeting threshold score (integer)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attributeGroup ref="hmis:baseMetaData"/>
    </xsd:complexType>

    <xsd:complexType name="event">
