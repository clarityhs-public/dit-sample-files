            </xsd:element>
            <xsd:element name="Notes" type="xsd:string" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">End Date</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AttendanceId" type="hmis:string32" minOccurs="0">
