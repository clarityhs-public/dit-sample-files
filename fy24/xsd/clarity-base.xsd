    <xsd:complexType name="disabilityResponseSubstanceUseDisorder" id="_4.10.2">
        <xsd:annotation>
            <xsd:documentation>Substance Use Disorder</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:restriction base="hmis:disabilityResponseBase">
                <xsd:enumeration value="0">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">No</xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="1">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">Alcohol use disorder</xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="2">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">Drug use disorder</xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="3">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">Both alcohol and drug use disorders
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="8">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">Client doesn’t know</xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="9">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">Client prefers not to answer
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="99">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">Data not collected</xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
            </xsd:restriction>
        </xsd:simpleContent>
