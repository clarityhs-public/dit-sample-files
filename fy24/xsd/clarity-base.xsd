    <xsd:attribute name="hashStatus">
        <xsd:simpleType>
            <xsd:restriction base="xsd:unsignedInt">
                <xsd:enumeration value="1">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">Unhashed</xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="2">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">SHA-1 (no longer used for federal
                            purposes)
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="3">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">Hashed - other</xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="4">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">SHA-256 (RHY)</xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:attribute>

    <xsd:attribute name="informationDate" id="_5.04.1" type="xsd:date">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Information Date (date information was collected) ([date
                field])
            </xsd:documentation>
        </xsd:annotation>
    </xsd:attribute>

    <xsd:attributeGroup name="baseMetaData">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">See FY2024 HMIS Data Dictionary, Section 5.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="dateCreated" id="_5.01.1" type="xsd:dateTime" use="required">
            <xsd:annotation>
                <xsd:documentation xml:lang="en">Date Created</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="dateDeleted" type="xsd:dateTime" use="optional">
            <xsd:annotation>
                <xsd:documentation xml:lang="en">If the intended record is to be deleted, this
                    metadata allows you to both indicate the record should be deleted within the
                    target system, as well as indicate the specific time it was deleted.
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="dateUpdated" id="_5.02.1" type="xsd:dateTime" use="required">
            <xsd:annotation>
                <xsd:documentation xml:lang="en">Date Updated</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="userID" id="_5.07.1" type="hmis:string32" use="required">
            <xsd:annotation>
                <xsd:documentation xml:lang="en">A unique ID used to associate data with the user who
                    entered and/or edited it
                </xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:attributeGroup>

    <xsd:attribute name="dataCollectionStage" id="_5.03.1">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">Data Collection Stage</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
            <xsd:restriction base="xsd:unsignedInt">
                <xsd:enumeration value="1">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">Project entry</xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="2">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">Project update</xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="3">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">Project exit</xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="5">
                    <xsd:annotation>
                        <xsd:documentation xml:lang="en">Project annual assessment</xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:attribute>

    <xsd:simpleType name="aftercareProvided" id="aftercareProvided">
        <xsd:restriction base="xsd:unsignedInt">
            <xsd:enumeration value="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">No</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Yes</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="9">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Client prefers not to answer</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="annualPercentAMI" id="annualPercentAMI">
        <xsd:restriction base="xsd:unsignedInt">
            <xsd:enumeration value="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">$0 (i.e., not employed, not receiving cash
                        benefits, no other current income)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">1-14% of Area Median Income (AMI) for household
                        size
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">15-30% of AMI for household size
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">More than 30% of AMI for household size
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Data not collected</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="assessmentLevel" id="assessmentLevel">
        <xsd:restriction base="xsd:unsignedInt">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Crisis Needs Assessment</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Housing Needs Assessment</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="assessmentType" id="assessmentType">
        <xsd:restriction base="xsd:unsignedInt">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Phone</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Virtual</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">In person</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="availability" id="availability">
        <xsd:restriction base="xsd:unsignedInt">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Year-round</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Seasonal</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Overflow</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="bedType" id="bedType">
        <xsd:restriction base="xsd:unsignedInt">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Facility-based beds</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Voucher beds</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Other beds</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="cmExitReason" id="cmExitReason">
        <xsd:restriction base="xsd:unsignedInt">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Accomplished goals and/or obtained services and
                        no longer needs CM
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Transferred to another HUD-VASH program site
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Found/chose other housing</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="4">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Did not comply with HUD-VASH CM
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="5">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Eviction and/or other housing related issues
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="6">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Unhappy with HUD-VASH housing</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="7">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">No longer financially eligible for HUD/VASH
                        voucher
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="8">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">No longer interested in participating in this
                        program
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="9">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Veteran cannot be located</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="10">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Veteran too ill to participate at this time
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="11">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Veteran is incarcerated</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="12">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Veteran is deceased</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="13">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Other</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="cocCode" id="cocCode">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Za-z]{2}-[0-9]{3}"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="countOfExchangeForSex" id="countOfExchangeForSex">
        <xsd:restriction base="xsd:unsignedInt">
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">1-3</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">4-7</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">8-11</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="4">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">12 or more</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="8">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Client doesn’t know</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="9">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Client prefers not to answer</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Data not collected</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="currentEdStatus" id="currentEdStatus">
        <xsd:restriction base="xsd:unsignedInt">
            <xsd:enumeration value="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Pursuing a high school diploma or GED
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Pursuing Associate’s Degree</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="2">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Pursuing Bachelor’s Degree</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="3">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Pursuing Graduate Degree</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="4">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Pursuing other post-secondary credential
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="8">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Client doesn’t know</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="9">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Client prefers not to answer</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="99">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Data not collected</xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="currentSchoolAttend" id="currentSchoolAttend">
        <xsd:restriction base="xsd:unsignedInt">
            <xsd:enumeration value="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Not currently enrolled in any school or
                        educational course
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
            <xsd:enumeration value="1">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Currently enrolled but NOT attending regularly
                        (when school or the course is in session)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:enumeration>
