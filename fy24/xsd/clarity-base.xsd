    <xsd:complexType name="disabilities">
        <xsd:sequence>
            <xsd:element name="DisabilitiesID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Reference to the Exit's unique ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EnrollmentID" type="hmis:enrollmentID"/>
            <xsd:element name="DisabilityType" type="hmis:disabilityType">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">The permissible values correspond to the decimal
                        values of the corresponding disability types. 4.05 Physical Disability is
                        "5" and 4.10 Substance Use Disorder is "10".
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="DisabilityResponse" type="hmis:disabilityResponseBase">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">See FY2024 HMIS Data Dictionary, Section
                        4.05-4.10, Response #2. Permissable values vary depending on which
                        disability type is declared. The values are contained with "Field and
                        Response(s) 2" within 4.5 through 4.10.
                    </xsd:documentation>
                    <xsd:documentation xml:lang="en">In the instance document, the xsi:type of this
                        element must be declared, and the type depends on the value within the
                        sibling hmis:DisabilityType element. See the example HUD_HMIS_Instance.xml
                        file to see how this should appear. Either
                        xsi:type="hmis:disabilityResponse[insert type]" or
                        xsi:type="hmis:disabilityResponseSubstanceUseDisorder"can be used.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="IndefiniteAndImpairs" type="hmis:indefiniteAndImpairsResponseBase"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">See FY2024 HMIS Data Dictionary, Section 4.05,
                        4.07, 4.09, and 4.10, Response A.
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="TCellCountAvailable" id="_W4.2" type="hmis:noYesReasonsForMissingData"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">T-Cell (CD4) Count Available</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="TCellCount" id="_W4.A" type="hmis:tCellCount" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">If a yes to “T-Cell (CD4) Count Available” then
                        T-cell Count (integer between 0 – 1500)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="TCellSource" id="_W4.B" type="hmis:tCellOrViralLoadSource"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">If a number is entered in the T-Cell (CD4)
                        count, then how was the information obtained?
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ViralLoadAvailable" id="_W4.3" type="hmis:viralLoadAvailable"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Viral Load Information Available
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ViralLoad" id="_W4.C" type="hmis:viralLoad" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">If “Viral Load Information Available” then count
                        (integer between 0 – 999999)
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="ViralLoadSource" id="_W4.D" type="hmis:tCellOrViralLoadSource"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">How was the information obtained?
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="AntiRetroviral" id="_W6.2" type="hmis:noYesReasonsForMissingData"
                        minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Has the participant been prescribed
                        anti-retroviral drugs?
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute ref="hmis:dataCollectionStage" use="required"/>
        <xsd:attribute ref="hmis:informationDate" use="required"/>
        <xsd:attributeGroup ref="hmis:baseMetaData"/>
    </xsd:complexType>

