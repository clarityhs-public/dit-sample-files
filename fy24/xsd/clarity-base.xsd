    <xsd:complexType name="exit">
        <xsd:sequence>
            <xsd:element name="ExitID" type="hmis:string32">
                <xsd:annotation>
                    <xsd:documentation xml:lang="en">Reference to the Exit's unique ID
                    </xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="EnrollmentID" type="hmis:enrollmentID"/>
